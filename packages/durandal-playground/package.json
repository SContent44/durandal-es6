{
    "name": "durandal-playground",
    "version": "3.0.0",
    "description": "Default starter website that demonstrates behaviour of using Durandal for building a Single Page Application (with Webpack).",
    "main": "index.html",
    "scripts": {
        "lint": "eslint ./** --config ../../.eslintrc.js --ext .js",
        "serve": "webpack-dev-server -d",
        "build": "webpack --prod",
        "webpack-stats": "webpack --profile --json > stats.json",
        "webpack-analyze": "webpack-bundle-analyzer stats.json"
    },
    "keywords": [
        "Durandal",
        "SPA",
        "Webpack",
        "Starter-Kit"
    ],
    "author": "Simon Buckley",
    "license": "ISC",
    "dependencies": {
        "@material/button": "^7.0.0",
        "@material/card": "^7.0.0",
        "@material/checkbox": "^7.0.0",
        "@material/circular-progress": "^7.0.0",
        "@material/dialog": "^7.0.0",
        "@material/drawer": "^7.0.0",
        "@material/fab": "^7.0.0",
        "@material/floating-label": "^7.0.0",
        "@material/form-field": "^7.0.0",
        "@material/icon-button": "^7.0.0",
        "@material/layout-grid": "^7.0.0",
        "@material/line-ripple": "^7.0.0",
        "@material/list": "^7.0.0",
        "@material/menu": "^7.0.0",
        "@material/notched-outline": "^7.0.0",
        "@material/ripple": "^7.0.0",
        "@material/snackbar": "^7.0.0",
        "@material/textfield": "^7.0.0",
        "@material/top-app-bar": "^7.0.0",
        "whatwg-fetch": "^3.0.0"
    }
}
